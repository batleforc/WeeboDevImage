FROM alpine/curl:latest

LABEL org.opencontainers.image.authors="batleforc"
LABEL org.opencontainers.image.url="https://github.com/batleforc/WeeboDevImage/alpine-jqcurl"
LABEL org.opencontainers.image.title="Alpine-jqcurl"
LABEL org.opencontainers.image.source = "https://github.com/batleforc/WeeboDevImage"

# Install jq
RUN apk add --no-cache jq